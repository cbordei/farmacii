.row
  = search_form_for @q do |f|
    .col-md-3
      = f.label :name_cont
      = f.search_field :name_cont 
    .col-md-3
      = f.label :county_cont
      = f.search_field :county_cont
    .col-md-3
      = f.label :city_cont
      = f.search_field :city_cont
    .col-md-3
      = f.submit :class => "btn btn-default"